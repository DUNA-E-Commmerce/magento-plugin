<config  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="sales_quote_address_collect_totals_before">
        <observer name="checkout_cart_quote" instance="DUna\Payments\Observer\CheckoutCartQuoteObserver"/>
    </event>

    <event name="sales_model_service_quote_submit_before">
        <observer name="entrepids_storepickup_quote_submit_before"  instance="DUna\Payments\Observer\Quote\SubmitBefore" />
    </event>

    <event name="sales_order_save_after">
        <observer name="order_update_observer" instance="DUna\Payments\Observer\OrderUpdateObserver" />
    </event>
    
    <event name="salesrule_validator_process">
        <observer name="duna_payments_apply_discount_observer" instance="DUna\Payments\Observer\ApplyDiscountObserver" />
    </event>

    <event name="sales_order_creditmemo_save_commit_after">
        <observer name="duna_payments_refund_observer" instance="DUna\Payments\Observer\RefundObserver" />
    </event>
</config>
